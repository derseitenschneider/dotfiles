{
	"title": "Change right cmd to be cmd+ctrl",
	"rules": [
		{
			"description": "Change right cmd to be cmd+ctrl",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "right_command",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_control",
							"modifiers": ["left_command"]
						}
					]
				}
			]
		}
	]
}
